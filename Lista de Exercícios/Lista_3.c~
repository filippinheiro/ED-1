#include <stdio.h>
#include <stdlib.h>
#include "pilha.h"
#include "fila.h"

float maior(Fila* fila){
	float maior;
	No* n = fila->ini;
	maior = n->info;
	for(n; n != NULL; n=n->prox) {
		if(n->info > maior)
			maior = n->info;
	}
	return maior;
}

int main(){
	Fila f1 = criarFila();
	inserir(&f1, 1);
	inserir(&f1, 5);
	inserir(&f1, 3);
	inserir(&f1, 8);
	inserir(&f1, 11);
	//imprimeFila(&f1);
	//printf("%.2f\n", maior(&f1));
	
	return 0;
	
		
}
